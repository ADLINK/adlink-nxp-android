diff --git a/imx9/lec_imx95/lec_imx95.mk b/imx9/lec_imx95/lec_imx95.mk
new file mode 100644
index 00000000..a033f859
--- /dev/null
+++ b/imx9/lec_imx95/lec_imx95.mk
@@ -0,0 +1,603 @@
+# -------@block_infrastructure-------
+CONFIG_REPO_PATH := device/nxp
+CURRENT_FILE_PATH :=  $(lastword $(MAKEFILE_LIST))
+IMX_DEVICE_PATH := $(strip $(patsubst %/, %, $(dir $(CURRENT_FILE_PATH))))
+
+PRODUCT_ENFORCE_ARTIFACT_PATH_REQUIREMENTS := true
+#Enable this to choose 32 bit user space build
+IMX_BUILD_32BIT_ROOTFS ?= false
+
+# configs shared between uboot, kernel and Android rootfs
+include $(IMX_DEVICE_PATH)/SharedBoardConfig.mk
+
+-include $(CONFIG_REPO_PATH)/common/imx_path/ImxPathConfig.mk
+include $(CONFIG_REPO_PATH)/imx9/ProductConfigCommon.mk
+
+# -------@block_common_config-------
+# Overrides
+PRODUCT_NAME := lec_imx95
+PRODUCT_DEVICE := lec_imx95
+PRODUCT_MODEL := LEC_IMX95
+
+TARGET_BOOTLOADER_BOARD_NAME := LEC
+
+PRODUCT_CHARACTERISTICS := tablet
+
+DEVICE_PACKAGE_OVERLAYS := $(IMX_DEVICE_PATH)/overlay
+
+PRODUCT_COMPATIBLE_PROPERTY_OVERRIDE := true
+
+PRODUCT_VENDOR_PROPERTIES += ro.soc.manufacturer=nxp
+PRODUCT_VENDOR_PROPERTIES += ro.soc.model=IMX95
