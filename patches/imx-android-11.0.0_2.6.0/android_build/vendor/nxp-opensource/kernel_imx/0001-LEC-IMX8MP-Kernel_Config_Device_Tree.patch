+	status = "okay";
+};
+
+&pwm1 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_pwm1>;
+	pwm-gpios = <&gpio8 8 GPIO_ACTIVE_LOW>;
+	status = "okay";
