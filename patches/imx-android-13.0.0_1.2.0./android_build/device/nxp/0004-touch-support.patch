From e7e227f31777149eb76af7deeb21cf15c157a87e Mon Sep 17 00:00:00 2001
From: jenkins <jenkins@adlinktech.com>
Date: Wed, 11 Jun 2025 01:09:37 +0000
Subject: [PATCH 4/9] touch support

Signed-off-by: jenkins <jenkins@adlinktech.com>
---
 imx8m/sp2_imx8mp/SharedBoardConfig.mk | 6 ++++--
 1 file changed, 4 insertions(+), 2 deletions(-)

diff --git a/imx8m/sp2_imx8mp/SharedBoardConfig.mk b/imx8m/sp2_imx8mp/SharedBoardConfig.mk
index d77912bc..9deb6df9 100644
--- a/imx8m/sp2_imx8mp/SharedBoardConfig.mk
+++ b/imx8m/sp2_imx8mp/SharedBoardConfig.mk
@@ -78,7 +78,8 @@ BOARD_VENDOR_KERNEL_MODULES += \
     $(KERNEL_OUT)/drivers/net/ethernet/freescale/fec.ko \
     $(KERNEL_OUT)/drivers/net/ethernet/stmicro/stmmac/dwmac-imx.ko \
     $(KERNEL_OUT)/drivers/net/ethernet/stmicro/stmmac/stmmac-platform.ko \
-    $(KERNEL_OUT)/drivers/net/ethernet/stmicro/stmmac/stmmac.ko
+    $(KERNEL_OUT)/drivers/net/ethernet/stmicro/stmmac/stmmac.ko \
+    $(KERNEL_OUT)/drivers/extcon/extcon-usb-gpio.ko
 ifeq ($(POWERSAVE),true)
 BOARD_VENDOR_KERNEL_MODULES += \
     $(KERNEL_OUT)/drivers/soc/imx/lpa_ctrl.ko \
@@ -226,7 +227,8 @@ BOARD_VENDOR_RAMDISK_KERNEL_MODULES +=     \
     $(KERNEL_OUT)/drivers/trusty/trusty-ipc.ko \
     $(KERNEL_OUT)/drivers/staging/media/imx/imx8-media-dev.ko \
     $(KERNEL_OUT)/net/wireless/cfg80211.ko \
-    $(KERNEL_OUT)/net/mac80211/mac80211.ko
+    $(KERNEL_OUT)/net/mac80211/mac80211.ko \
+    $(KERNEL_OUT)/drivers/gpu/drm/panel/panel-lvds.ko
 else
 BOARD_VENDOR_RAMDISK_KERNEL_MODULES +=     \
     $(KERNEL_OUT)/drivers/input/touchscreen/goodix.ko \
-- 
